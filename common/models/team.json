{
  "name": "Team",
  "plural": "teams",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "numPlayers": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "teacher": {
      "type": "belongsTo",
      "model": "Teacher",
      "foreignKey": "teacherId"
    },
    "competitions": {
      "type": "hasAndBelongsToMany",
      "model": "Competition",
      "foreignKey": "competitionId"
    },
    "students": {
      "type": "hasAndBelongsToMany",
      "model": "Student",
      "foreignKey": "studentId"
    }
  },
  "acls": [],
  "methods": {}
}
